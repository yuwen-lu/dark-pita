{
  "name": "Dark Pita",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "assets/logos/logo16.png",
      "48": "assets/logos/logo48.png",
      "128": "assets/logos/logo128.png"
    },
    "default_title": "Dark Pita"
  },
  "icons": {
    "16": "assets/logos/logo16.png",
    "48": "assets/logos/logo48.png",
    "128": "assets/logos/logo128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contents/main.js"]
    }
  ],
  "permissions": ["tabs", "identity", "identity.email"]
}
